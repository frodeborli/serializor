{
    "name": "frodeborli/serializor",
    "description": "PHP Serializor will serialize almost anything in PHP, including closures and anonymous classes.",
    "keywords": [
        "SerializableClosure",
        "serialization",
        "serialize",
        "opis closure"
    ],
    "type": "library",
    "require-dev": {
        "pestphp/pest": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Serializor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Frode BÃ¸rli",
            "email": "frode@ennerd.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "nikic/php-parser": "^5.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}